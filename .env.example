# QuantForge example .env
# Copy this file to .env and fill values before running.

# App / env
APP_NAME=QuantForge_AI_Engine
APP_VERSION=0.1.0
ENV=dev

# CORS (comma separated origins) - for quick local dev keep '*' or set explicit URLs in prod
ALLOWED_ORIGINS=["*"]

# Local Ollama / LLM runtime (defaults assume local Ollama REST)
OLLAMA_HOST=http://127.0.0.1
OLLAMA_PORT=11434

# External model keys (optional)
OPENAI_API_KEY=
HF_API_KEY=

# Databases (for Phase 1 you can keep these empty and use docker-compose)
DATABASE_URL=postgresql://postgres:password@localhost:5432/quantforge
REDIS_URL=redis://localhost:6379/0

# Cloudflare Turnstile (optional, for signup)
TURNSTILE_SITE_KEY=
TURNSTILE_SECRET=

# Logging
LOG_LEVEL=DEBUG
